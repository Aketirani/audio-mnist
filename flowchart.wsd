@startuml Pipeline Flowchart
|AudioMNIST|
package "AudioMNIST" {
    :start|
    partition "main.sh" {
        :set log path;
        :run python script;
        partition "main.py" {
            :import packages;
            :initialize classes;
            :prepare raw audio data for analysis;
            :prepare processed data for modelling;
            :train model and make predictions;
        }
        :write to log;
    }
    :end|
}
stop
@enduml
