@startuml Pipeline Flowchart
|audio_mnist|
package "audio-mnist" {
    :start|
    partition "audiomnist.sh" {
        :set repository path;
        :set log path;
        :run python script;
        partition "audiomnist.py" {
            :data preparation;
            :feature engineering;
            :data splitting;
            :model tuning;
            :model training;
            :model prediction;
        }
        :write to log;
    }
    :end|
}
stop
@enduml
